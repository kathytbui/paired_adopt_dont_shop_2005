<h1>Edit a Review</h1>
<%= form_tag "/shelters/#{@review.shelter_id}", id: "review-#{@review.id}", method: :patch do %>
<%= label_tag :title %>
<%= text_field_tag :title, @review.title %>
<%= label_tag :rating %>
<%= text_field_tag :rating, @review.rating%>
<%= label_tag :content %>
<%= text_field_tag :content, @review.content %>
<%= label_tag :picture %>
<%= text_field_tag :picture, @review.picture %>

<%= submit_tag "SUBMIT" %>
<% end %>
